{
  "hosting": {
    "public": ".",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/tantoshigostanto/glosario.html",
        "destination": "/glosario.html"
      },
      {
        "source": "/tantoshigostanto/blog.html",
        "destination": "/blog.html"
      },
      {
        "source": "/tantoshigostanto/generador.html",
        "destination": "/generador.html"
      },
      {
        "source": "/tantoshigostanto/index.html",
        "destination": "/index.html"
      }
    ]
  }
}
